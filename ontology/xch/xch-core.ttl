@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix prov: <http://www.w3.org/ns/prov#> .
@prefix xch: <https://w3id.org/xch-mind/ontology/> .
@prefix xch-res: <https://w3id.org/xch-mind/resource/> .
@prefix arco: <https://w3id.org/arco/ontology/arco/> .
@prefix arco-res: <https://w3id.org/arco/resource/> .

# =============================================================================
# ONTOLOGY METADATA
# =============================================================================

<https://w3id.org/xch-mind/ontology/>
    rdf:type owl:Ontology ;
    owl:versionIRI <https://w3id.org/xch-mind/ontology/0.1.0> ;
    dc:title "xch-MIND Ontology"@en ;
    dc:title "Ontologia xch-MIND"@it ;
    dc:description """
        The xch-MIND ontology provides classes and properties for 
        representing interpretive layers over cultural heritage knowledge bases.
        It is designed to augment existing ontologies (like ArCo) without modification,
        by adding inferred relationships, thematic paths, and provenance information
        generated by xch-MIND autonomous agents.
    """@en ;
    dc:description """
        L'ontologia xch-MIND fornisce classi e proprietà per 
        rappresentare livelli interpretativi sopra basi di conoscenza del patrimonio culturale.
        È progettata per arricchire ontologie esistenti (come ArCo) senza modificarle,
        aggiungendo relazioni inferite, percorsi tematici e informazioni di provenance
        generate dagli agenti autonomi di xch-MIND.
    """@it ;
    dc:creator "Michele Stingo" ;
    dcterms:created "2026-01-26"^^xsd:date ;
    dcterms:modified "2026-01-26"^^xsd:date ;
    owl:versionInfo "0.1.0" ;
    dcterms:license <https://creativecommons.org/licenses/by/4.0/> ;
    rdfs:comment "Ontology for augmented cultural heritage knowledge graphs"@en .


# =============================================================================
# CLASSES - Core Interpretive Assertions
# =============================================================================

xch:InterpretiveAssertion
    rdf:type owl:Class ;
    rdfs:label "Interpretive Assertion"@en ;
    rdfs:label "Asserzione Interpretativa"@it ;
    rdfs:comment """
        Superclass for all assertions generated by xch agents. 
        Every interpretive assertion carries provenance and confidence metadata.
    """@en ;
    rdfs:subClassOf prov:Entity .

xch:SpatialRelation
    rdf:type owl:Class ;
    rdfs:subClassOf xch:InterpretiveAssertion ;
    rdfs:label "Spatial Relation"@en ;
    rdfs:label "Relazione Spaziale"@it ;
    rdfs:comment "An inferred spatial relationship between cultural properties."@en .

xch:TemporalRelation
    rdf:type owl:Class ;
    rdfs:subClassOf xch:InterpretiveAssertion ;
    rdfs:label "Temporal Relation"@en ;
    rdfs:label "Relazione Temporale"@it ;
    rdfs:comment "An inferred temporal relationship between cultural properties."@en .

xch:TypologicalRelation
    rdf:type owl:Class ;
    rdfs:subClassOf xch:InterpretiveAssertion ;
    rdfs:label "Typological Relation"@en ;
    rdfs:label "Relazione Tipologica"@it ;
    rdfs:comment "An inferred typological similarity between cultural properties."@en .


# =============================================================================
# CLASSES - Clusters and Groups
# =============================================================================

xch:Cluster
    rdf:type owl:Class ;
    rdfs:subClassOf xch:InterpretiveAssertion ;
    rdfs:label "Cluster"@en ;
    rdfs:label "Cluster"@it ;
    rdfs:comment "A group of related cultural properties identified through analysis."@en .

xch:GeographicCluster
    rdf:type owl:Class ;
    rdfs:subClassOf xch:Cluster ;
    rdfs:label "Geographic Cluster"@en ;
    rdfs:label "Cluster Geografico"@it ;
    rdfs:comment "A group of cultural properties clustered by geographic proximity."@en .

xch:ChronologicalCluster
    rdf:type owl:Class ;
    rdfs:subClassOf xch:Cluster ;
    rdfs:label "Chronological Cluster"@en ;
    rdfs:label "Cluster Cronologico"@it ;
    rdfs:comment "A group of cultural properties clustered by temporal period."@en .

xch:TypologicalCluster
    rdf:type owl:Class ;
    rdfs:subClassOf xch:Cluster ;
    rdfs:label "Typological Cluster"@en ;
    rdfs:label "Cluster Tipologico"@it ;
    rdfs:comment "A group of cultural properties clustered by architectural or structural features."@en .


# =============================================================================
# CLASSES - Thematic Paths
# =============================================================================

xch:ThematicPath
    rdf:type owl:Class ;
    rdfs:subClassOf xch:InterpretiveAssertion ;
    rdfs:label "Thematic Path"@en ;
    rdfs:label "Percorso Tematico"@it ;
    rdfs:comment """
        A curated sequence of cultural properties connected by a theme, 
        narrative, or visiting itinerary.
    """@en .

xch:GeographicPath
    rdf:type owl:Class ;
    rdfs:subClassOf xch:ThematicPath ;
    rdfs:label "Geographic Path"@en ;
    rdfs:label "Percorso Geografico"@it ;
    rdfs:comment "A path optimized for geographic visiting sequence."@en .

xch:ChronologicalPath
    rdf:type owl:Class ;
    rdfs:subClassOf xch:ThematicPath ;
    rdfs:label "Chronological Path"@en ;
    rdfs:label "Percorso Cronologico"@it ;
    rdfs:comment "A path ordered by historical period."@en .

xch:TypologicalPath
    rdf:type owl:Class ;
    rdfs:subClassOf xch:ThematicPath ;
    rdfs:label "Typological Path"@en ;
    rdfs:label "Percorso Tipologico"@it ;
    rdfs:comment "A path connecting sites that share typological or architectural features."@en .

xch:NarrativePath
    rdf:type owl:Class ;
    rdfs:subClassOf xch:ThematicPath ;
    rdfs:label "Narrative Path"@en ;
    rdfs:label "Percorso Narrativo"@it ;
    rdfs:comment "A path with a storytelling structure combining multiple criteria (geographic, chronological, typological)."@en .

xch:PathStop
    rdf:type owl:Class ;
    rdfs:label "Path Stop"@en ;
    rdfs:label "Tappa del Percorso"@it ;
    rdfs:comment "A single stop within a thematic path, with order and narrative context."@en .


# =============================================================================
# CLASSES - Features and Characteristics
# =============================================================================

xch:ExtractedFeature
    rdf:type owl:Class ;
    rdfs:subClassOf xch:InterpretiveAssertion ;
    rdfs:label "Extracted Feature"@en ;
    rdfs:label "Caratteristica Estratta"@it ;
    rdfs:comment "A feature or characteristic extracted from source descriptions by agents."@en .

xch:ArchitecturalFeature
    rdf:type owl:Class ;
    rdfs:subClassOf xch:ExtractedFeature ;
    rdfs:label "Architectural Feature"@en ;
    rdfs:label "Caratteristica Architettonica"@it ;
    rdfs:comment "An architectural or structural characteristic."@en .

xch:FunctionalFeature
    rdf:type owl:Class ;
    rdfs:subClassOf xch:ExtractedFeature ;
    rdfs:label "Functional Feature"@en ;
    rdfs:label "Caratteristica Funzionale"@it ;
    rdfs:comment "A feature related to the function or purpose of a cultural property."@en .


# =============================================================================
# CLASSES - Agents and Provenance
# =============================================================================

xch:Agent
    rdf:type owl:Class ;
    rdfs:subClassOf prov:Agent ;
    rdfs:label "xch Agent"@en ;
    rdfs:label "Agente xch"@it ;
    rdfs:comment "An autonomous agent that generates interpretive assertions."@en .

xch:OrchestratorAgent
    rdf:type owl:Class ;
    rdfs:subClassOf xch:Agent ;
    rdfs:label "Orchestrator Agent"@en ;
    rdfs:label "Agente Orchestratore"@it ;
    rdfs:comment "The central coordinating agent that routes tasks to specialized agents."@en .

xch:GeoSpatialAgent
    rdf:type owl:Class ;
    rdfs:subClassOf xch:Agent ;
    rdfs:label "GeoSpatial Agent"@en ;
    rdfs:label "Agente GeoSpaziale"@it ;
    rdfs:comment "Agent specialized in geographic and spatial analysis."@en .

xch:ChronologicalAgent
    rdf:type owl:Class ;
    rdfs:subClassOf xch:Agent ;
    rdfs:label "Chronological Agent"@en ;
    rdfs:label "Agente Cronologico"@it ;
    rdfs:comment "Agent specialized in temporal and dating analysis."@en .

xch:TypologicalAgent
    rdf:type owl:Class ;
    rdfs:subClassOf xch:Agent ;
    rdfs:label "Typological Agent"@en ;
    rdfs:label "Agente Tipologico"@it ;
    rdfs:comment "Agent specialized in typological and morphological analysis."@en .

xch:NarrativeAgent
    rdf:type owl:Class ;
    rdfs:subClassOf xch:Agent ;
    rdfs:label "Narrative Agent"@en ;
    rdfs:label "Agente Narrativo"@it ;
    rdfs:comment "Agent specialized in generating narrative paths and descriptions."@en .

xch:ValidationAgent
    rdf:type owl:Class ;
    rdfs:subClassOf xch:Agent ;
    rdfs:label "Validation Agent"@en ;
    rdfs:label "Agente di Validazione"@it ;
    rdfs:comment "Agent specialized in validating assertions against source data."@en .


# =============================================================================
# CLASSES - Validation
# =============================================================================

xch:ValidationStatus
    rdf:type owl:Class ;
    rdfs:label "Validation Status"@en ;
    rdfs:label "Stato di Validazione"@it ;
    rdfs:comment "The validation status of an interpretive assertion."@en .

xch:Validated
    rdf:type xch:ValidationStatus ;
    rdfs:label "Validated"@en ;
    rdfs:label "Validato"@it ;
    rdfs:comment "The assertion has been validated automatically."@en .

xch:PendingReview
    rdf:type xch:ValidationStatus ;
    rdfs:label "Pending Review"@en ;
    rdfs:label "In Attesa di Revisione"@it ;
    rdfs:comment "The assertion requires manual review due to low confidence."@en .

xch:ManuallyValidated
    rdf:type xch:ValidationStatus ;
    rdfs:label "Manually Validated"@en ;
    rdfs:label "Validato Manualmente"@it ;
    rdfs:comment "The assertion has been validated by human review."@en .

xch:Rejected
    rdf:type xch:ValidationStatus ;
    rdfs:label "Rejected"@en ;
    rdfs:label "Rifiutato"@it ;
    rdfs:comment "The assertion has been rejected as invalid."@en .


# =============================================================================
# OBJECT PROPERTIES - Core Relations
# =============================================================================

xch:groundedOn
    rdf:type owl:ObjectProperty ;
    rdfs:subPropertyOf prov:used ;
    rdfs:domain xch:InterpretiveAssertion ;
    rdfs:range owl:Thing ;
    rdfs:label "grounded on"@en ;
    rdfs:label "basato su"@it ;
    rdfs:comment "Links an interpretive assertion to the source entity it is based on. SubProperty of prov:used for PROV-O interoperability."@en .

xch:includesSite
    rdf:type owl:ObjectProperty ;
    rdfs:domain [ owl:unionOf (xch:Cluster xch:ThematicPath) ] ;
    rdfs:range owl:Thing ;
    rdfs:label "includes site"@en ;
    rdfs:label "include sito"@it ;
    rdfs:comment "Links a cluster or path to a cultural property it includes."@en .

xch:relatesTo
    rdf:type owl:ObjectProperty ;
    rdfs:domain xch:InterpretiveAssertion ;
    rdfs:range owl:Thing ;
    rdfs:label "relates to"@en ;
    rdfs:label "si riferisce a"@it ;
    rdfs:comment "Generic relation between an assertion and an entity."@en .

xch:similarTo
    rdf:type owl:ObjectProperty ;
    rdfs:subPropertyOf xch:relatesTo ;
    rdfs:label "similar to"@en ;
    rdfs:label "simile a"@it ;
    rdfs:comment "Indicates typological or structural similarity between entities."@en .

xch:nearTo
    rdf:type owl:ObjectProperty ;
    rdfs:subPropertyOf xch:relatesTo ;
    rdfs:label "near to"@en ;
    rdfs:label "vicino a"@it ;
    rdfs:comment "Indicates geographic proximity between entities."@en .

xch:contemporaryWith
    rdf:type owl:ObjectProperty ;
    rdfs:subPropertyOf xch:relatesTo ;
    rdfs:label "contemporary with"@en ;
    rdfs:label "contemporaneo a"@it ;
    rdfs:comment "Indicates temporal overlap or contemporaneity between entities."@en .


# =============================================================================
# OBJECT PROPERTIES - Path Structure
# =============================================================================

xch:hasStop
    rdf:type owl:ObjectProperty ;
    rdfs:domain xch:ThematicPath ;
    rdfs:range xch:PathStop ;
    rdfs:label "has stop"@en ;
    rdfs:label "ha tappa"@it ;
    rdfs:comment "Links a thematic path to one of its stops."@en .

xch:stopSite
    rdf:type owl:ObjectProperty ;
    rdfs:domain xch:PathStop ;
    rdfs:range owl:Thing ;
    rdfs:label "stop site"@en ;
    rdfs:label "sito della tappa"@it ;
    rdfs:comment "Links a path stop to the cultural property it represents."@en .

xch:nextStop
    rdf:type owl:ObjectProperty ;
    rdfs:domain xch:PathStop ;
    rdfs:range xch:PathStop ;
    rdfs:label "next stop"@en ;
    rdfs:label "prossima tappa"@it ;
    rdfs:comment "Links a path stop to the next stop in the sequence."@en .

xch:hasTheme
    rdf:type owl:ObjectProperty ;
    rdfs:domain xch:ThematicPath ;
    rdfs:range skos:Concept ;
    rdfs:label "has theme"@en ;
    rdfs:label "ha tema"@it ;
    rdfs:comment "Links a thematic path to its main theme or topic."@en .


# =============================================================================
# OBJECT PROPERTIES - Features
# =============================================================================

xch:hasFeature
    rdf:type owl:ObjectProperty ;
    rdfs:range xch:ExtractedFeature ;
    rdfs:label "has feature"@en ;
    rdfs:label "ha caratteristica"@it ;
    rdfs:comment "Links an entity to an extracted feature."@en .

xch:featureOf
    rdf:type owl:ObjectProperty ;
    owl:inverseOf xch:hasFeature ;
    rdfs:domain xch:ExtractedFeature ;
    rdfs:label "feature of"@en ;
    rdfs:label "caratteristica di"@it ;
    rdfs:comment "Links an extracted feature to the entity it describes."@en .


# =============================================================================
# OBJECT PROPERTIES - Relation Reification
# =============================================================================

xch:source
    rdf:type owl:ObjectProperty ;
    rdfs:domain xch:InterpretiveAssertion ;
    rdfs:range owl:Thing ;
    rdfs:label "source"@en ;
    rdfs:label "sorgente"@it ;
    rdfs:comment "Source entity of a reified relation."@en .

xch:target
    rdf:type owl:ObjectProperty ;
    rdfs:domain xch:InterpretiveAssertion ;
    rdfs:range owl:Thing ;
    rdfs:label "target"@en ;
    rdfs:label "destinazione"@it ;
    rdfs:comment "Target entity of a reified relation."@en .

xch:belongsToPath
    rdf:type owl:ObjectProperty ;
    owl:inverseOf xch:hasStop ;
    rdfs:domain xch:PathStop ;
    rdfs:range xch:ThematicPath ;
    rdfs:label "belongs to path"@en ;
    rdfs:label "appartiene al percorso"@it ;
    rdfs:comment "Links a path stop to its parent thematic path."@en .

xch:about
    rdf:type owl:ObjectProperty ;
    rdfs:subPropertyOf xch:relatesTo ;
    rdfs:domain xch:InterpretiveAssertion ;
    rdfs:range owl:Thing ;
    rdfs:label "about"@en ;
    rdfs:label "riguarda"@it ;
    rdfs:comment "Generic relation linking an assertion to an entity it is about."@en .


# =============================================================================
# OBJECT PROPERTIES - Provenance
# =============================================================================

xch:generatedBy
    rdf:type owl:ObjectProperty ;
    rdfs:subPropertyOf prov:wasGeneratedBy ;
    rdfs:domain xch:InterpretiveAssertion ;
    rdfs:range xch:Agent ;
    rdfs:label "generated by"@en ;
    rdfs:label "generato da"@it ;
    rdfs:comment "Links an assertion to the agent that generated it."@en .

xch:validatedBy
    rdf:type owl:ObjectProperty ;
    rdfs:domain xch:InterpretiveAssertion ;
    rdfs:range xch:Agent ;
    rdfs:label "validated by"@en ;
    rdfs:label "validato da"@it ;
    rdfs:comment "Links an assertion to the agent that validated it."@en .

xch:hasValidationStatus
    rdf:type owl:ObjectProperty ;
    rdfs:domain xch:InterpretiveAssertion ;
    rdfs:range xch:ValidationStatus ;
    rdfs:label "has validation status"@en ;
    rdfs:label "ha stato di validazione"@it ;
    rdfs:comment "Links an assertion to its validation status."@en .

xch:derivedFrom
    rdf:type owl:ObjectProperty ;
    rdfs:subPropertyOf prov:wasDerivedFrom ;
    rdfs:domain xch:InterpretiveAssertion ;
    rdfs:label "derived from"@en ;
    rdfs:label "derivato da"@it ;
    rdfs:comment "Links an assertion to its source data or previous assertion."@en .


# =============================================================================
# DATATYPE PROPERTIES
# =============================================================================

xch:confidenceScore
    rdf:type owl:DatatypeProperty ;
    rdfs:domain xch:InterpretiveAssertion ;
    rdfs:range xsd:decimal ;
    rdfs:label "confidence score"@en ;
    rdfs:label "punteggio di confidenza"@it ;
    rdfs:comment "A confidence score between 0 and 1 indicating reliability of the assertion."@en .

xch:generatedAt
    rdf:type owl:DatatypeProperty ;
    rdfs:domain xch:InterpretiveAssertion ;
    rdfs:range xsd:dateTime ;
    rdfs:label "generated at"@en ;
    rdfs:label "generato il"@it ;
    rdfs:comment "Timestamp when the assertion was generated."@en .

xch:validatedAt
    rdf:type owl:DatatypeProperty ;
    rdfs:domain xch:InterpretiveAssertion ;
    rdfs:range xsd:dateTime ;
    rdfs:label "validated at"@en ;
    rdfs:label "validato il"@it ;
    rdfs:comment "Timestamp when the assertion was validated."@en .

xch:reasoningTrace
    rdf:type owl:DatatypeProperty ;
    rdfs:domain xch:InterpretiveAssertion ;
    rdfs:range xsd:string ;
    rdfs:label "reasoning trace"@en ;
    rdfs:label "traccia del ragionamento"@it ;
    rdfs:comment "Explanation of the reasoning that led to this assertion."@en .

xch:validationNotes
    rdf:type owl:DatatypeProperty ;
    rdfs:domain xch:InterpretiveAssertion ;
    rdfs:range xsd:string ;
    rdfs:label "validation notes"@en ;
    rdfs:label "note di validazione"@it ;
    rdfs:comment "Notes from the validation process."@en .


# =============================================================================
# DATATYPE PROPERTIES - Path-specific
# =============================================================================

xch:stopOrder
    rdf:type owl:DatatypeProperty ;
    rdfs:domain xch:PathStop ;
    rdfs:range xsd:integer ;
    rdfs:label "stop order"@en ;
    rdfs:label "ordine della tappa"@it ;
    rdfs:comment "The position of this stop in the path sequence (1-based)."@en .

xch:narrativeSnippet
    rdf:type owl:DatatypeProperty ;
    rdfs:domain xch:PathStop ;
    rdfs:range xsd:string ;
    rdfs:label "narrative snippet"@en ;
    rdfs:label "frammento narrativo"@it ;
    rdfs:comment "A short narrative text for this stop in the path."@en .

xch:connectionReason
    rdf:type owl:DatatypeProperty ;
    rdfs:domain xch:PathStop ;
    rdfs:range xsd:string ;
    rdfs:label "connection reason"@en ;
    rdfs:label "motivo della connessione"@it ;
    rdfs:comment "Explanation of why this stop connects to the next."@en .

xch:estimatedDuration
    rdf:type owl:DatatypeProperty ;
    rdfs:domain xch:ThematicPath ;
    rdfs:range xsd:duration ;
    rdfs:label "estimated duration"@en ;
    rdfs:label "durata stimata"@it ;
    rdfs:comment "Estimated time to complete the path."@en .

xch:pathLength
    rdf:type owl:DatatypeProperty ;
    rdfs:domain xch:ThematicPath ;
    rdfs:range xsd:integer ;
    rdfs:label "path length"@en ;
    rdfs:label "lunghezza del percorso"@it ;
    rdfs:comment "Number of stops in the path."@en .


# =============================================================================
# DATATYPE PROPERTIES - Spatial
# =============================================================================

xch:distanceKm
    rdf:type owl:DatatypeProperty ;
    rdfs:domain xch:SpatialRelation ;
    rdfs:range xsd:decimal ;
    rdfs:label "distance in km"@en ;
    rdfs:label "distanza in km"@it ;
    rdfs:comment "Distance in kilometers between related entities."@en .

xch:regionName
    rdf:type owl:DatatypeProperty ;
    rdfs:domain xch:GeographicCluster ;
    rdfs:range xsd:string ;
    rdfs:label "region name"@en ;
    rdfs:label "nome regione"@it ;
    rdfs:comment "The geographic region name for this cluster."@en .


# =============================================================================
# DATATYPE PROPERTIES - Temporal
# =============================================================================

xch:periodLabel
    rdf:type owl:DatatypeProperty ;
    rdfs:domain xch:ChronologicalCluster ;
    rdfs:range xsd:string ;
    rdfs:label "period label"@en ;
    rdfs:label "etichetta del periodo"@it ;
    rdfs:comment "Human-readable label for the time period."@en .

xch:startYear
    rdf:type owl:DatatypeProperty ;
    rdfs:domain xch:ChronologicalCluster ;
    rdfs:range xsd:integer ;
    rdfs:label "start year"@en ;
    rdfs:label "anno di inizio"@it ;
    rdfs:comment "Starting year of the period (negative for BCE)."@en .

xch:endYear
    rdf:type owl:DatatypeProperty ;
    rdfs:domain xch:ChronologicalCluster ;
    rdfs:range xsd:integer ;
    rdfs:label "end year"@en ;
    rdfs:label "anno di fine"@it ;
    rdfs:comment "Ending year of the period (negative for BCE)."@en .


# =============================================================================
# DATATYPE PROPERTIES - Typological
# =============================================================================

xch:typologyLabel
    rdf:type owl:DatatypeProperty ;
    rdfs:domain xch:TypologicalCluster ;
    rdfs:range xsd:string ;
    rdfs:label "typology label"@en ;
    rdfs:label "etichetta tipologica"@it ;
    rdfs:comment "Label describing the typological classification."@en .

xch:featureLabel
    rdf:type owl:DatatypeProperty ;
    rdfs:domain xch:ExtractedFeature ;
    rdfs:range xsd:string ;
    rdfs:label "feature label"@en ;
    rdfs:label "etichetta caratteristica"@it ;
    rdfs:comment "Label for the extracted feature."@en .

xch:similarityScore
    rdf:type owl:DatatypeProperty ;
    rdfs:domain xch:TypologicalRelation ;
    rdfs:range xsd:decimal ;
    rdfs:label "similarity score"@en ;
    rdfs:label "punteggio di similarità"@it ;
    rdfs:comment "A score indicating degree of typological similarity."@en .


# =============================================================================
# DATATYPE PROPERTIES - Additional
# =============================================================================

xch:temporalExtent
    rdf:type owl:DatatypeProperty ;
    rdfs:domain xch:ChronologicalCluster ;
    rdfs:range xsd:string ;
    rdfs:label "temporal extent"@en ;
    rdfs:label "estensione temporale"@it ;
    rdfs:comment "Human-readable temporal extent string (e.g. '-3000 to -1500')."@en .

xch:definingFeature
    rdf:type owl:DatatypeProperty ;
    rdfs:domain xch:TypologicalCluster ;
    rdfs:range xsd:string ;
    rdfs:label "defining feature"@en ;
    rdfs:label "caratteristica distintiva"@it ;
    rdfs:comment "A shared feature that defines a typological cluster."@en .

xch:difficulty
    rdf:type owl:DatatypeProperty ;
    rdfs:domain xch:ThematicPath ;
    rdfs:range xsd:string ;
    rdfs:label "difficulty"@en ;
    rdfs:label "difficoltà"@it ;
    rdfs:comment "Difficulty level of a thematic path (easy, medium, hard)."@en .

xch:narrative
    rdf:type owl:DatatypeProperty ;
    rdfs:domain xch:ThematicPath ;
    rdfs:range xsd:string ;
    rdfs:label "narrative"@en ;
    rdfs:label "narrativa"@it ;
    rdfs:comment "Overall narrative text for a thematic path."@en .


# =============================================================================
# AGENT INSTANCES (Standard agents used in the system)
# =============================================================================

xch-res:OrchestratorAgent
    rdf:type xch:OrchestratorAgent ;
    rdfs:label "xch Orchestrator Agent"@en ;
    rdfs:comment "The main orchestrator agent instance."@en .

xch-res:GeoSpatialAgent
    rdf:type xch:GeoSpatialAgent ;
    rdfs:label "xch GeoSpatial Agent"@en ;
    rdfs:comment "The geospatial analysis agent instance."@en .

xch-res:ChronologicalAgent
    rdf:type xch:ChronologicalAgent ;
    rdfs:label "xch Chronological Agent"@en ;
    rdfs:comment "The chronological analysis agent instance."@en .

xch-res:TypologicalAgent
