    <rdfs:label xml:lang="en">dolmen, Dolmen Grassi (PERIODIZZAZIONI/ PROTOSTORIA/ Età del Bronzo/ Età del Bronzo antico)</rdfs:label>
    <rdfs:label xml:lang="it">dolmen, Dolmen Grassi (PERIODIZZAZIONI/ PROTOSTORIA/ Età del Bronzo/ Età del Bronzo antico)</rdfs:label>
    <a-loc:hasCulturalPropertyAddress rdf:resource="https://w3id.org/arco/resource/Address/2732ae24a43ad235787cd4e1b8aad7ec"/>
